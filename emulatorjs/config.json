{
  "name": "EmulatorJS",
  "version": "1.0.0",
  "slug": "emulatorjs",
  "description": "Builds EmulatorJS directly from GitHub source.",
  "arch": ["amd64", "aarch64"],
  "startup": "services",
  "boot": "auto",
  "webui": "http://[HOST]:[PORT:8080]",
  "ports": {
    "8080/tcp": 8080
  },
  "map": [
  "/media/JELLYFINMEDIA/EmulatorJS/roms:ro:/roms",
  "/media/JELLYFINMEDIA/EmulatorJS/saves:/emulatorjs/saves",
  "/media/JELLYFINMEDIA/EmulatorJS/cores:/emulatorjs/cores"
]
}
